#: 400.php:12
msgid "错误请求"
msgstr "Bad Request"

#: 400.php:13
msgid "服务器不理解请求的语法"
msgstr "The server does not understand the syntax of the request"

#: 401.php:12
msgid "未授权"
msgstr "Unauthorized"

#: 401.php:13
msgid "对于需要登录的网页，服务器可能返回此响应"
msgstr "For web pages that need to be logged in, the server may return this response"

#: 403.php:12
msgid "禁止"
msgstr "Forbidden"

#: 403.php:13
msgid "服务器拒绝请求"
msgstr "Server refusal the request"

#: 404.php:12
msgid "页面未找到"
msgstr "Page Not Found"

#: 404.php:13
msgid "用户发出的请求针对的是不存在的页面"
msgstr "The user’s request is for a page that does not exist"

#: 405.php:12
msgid "方法禁用"
msgstr "Method Not Allowed"

#: 405.php:13
msgid "禁用请求中指定的方法"
msgstr "Disable the method specified in the request"

#: 422.php:12
msgid "无法处理的实体"
msgstr "Unprocessable Entity"

#: 422.php:13
msgid "请求格式正确，但是由于含有语义错误，无法响应"
msgstr "The request format is correct, but because of semantic errors, it cannot respond"

#: 429.php:12
msgid "太多请求"
msgstr "Too Many Requests"

#: 429.php:13
msgid "用户在给定的时间内发送了太多的请求"
msgstr "The user sends too many requests within a given time"

#: 500.php:12
msgid "服务器内部错误"
msgstr "Internal Server Error"

#: 500.php:13
msgid "服务器遇到错误，无法完成请求"
msgstr "Could not complete request"

#: layout.php:95
msgid "首页"
msgstr "Home"

#: layout.php:96
msgid "重试"
msgstr "Retry"
